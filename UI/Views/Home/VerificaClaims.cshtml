
@{
    ViewBag.Title = "VerificaClaims";
}

<h2>VerificaClaims</h2>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Valor</th>
        </tr>
    </thead>
    <tbody>
        @{foreach (var item in ViewBag.Claims)
            {
                <tr>
                    <td>@item.Key.Substring(1, 10)</td>
                    <td>@item.Value</td>
                </tr>
            }
        }
    </tbody>
</table>